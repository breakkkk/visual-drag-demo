{
  "name": "visual-drag-demo",
  "version": "0.1.0",
  "private": false,
  "main": "lib/h5-editor.umd.js",
  "style": "lib/designer.style.css",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:form-render": "vite build --config vite.config.form-render.ts",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,vue,json,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "ace-builds": "^1.12.3",
    "core-js": "^3.8.3",
    "echarts": "^5.4.1",
    "element-plus": "^2.13.1",
    "html-to-image": "^1.9.0",
    "mathjs": "^10.5.3",
    "nanoid": "^4.0.0",
    "pinia": "^2.3.1",
    "vue": "^3.3.6",
    "vue-echarts": "^6.5.4",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "chalk": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^3.3.3",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^4.5.14",
    "vue-tsc": "^3.2.4"
  }
}
